    "expo": "~54.0.32",
    "expo-av": "~16.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-device": "~8.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-nearby-connections": "^1.0.0",
    "expo-status-bar": "~3.0.9",
    "lucide-react-native": "^0.562.0",
