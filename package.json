{
  "name": "reactnative",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android --device Pixel_7 && expo run:android --device 2410CRP4CG --no-bundler && expo run:android --device CPH2399 --no-bundler",
    "android:pixel": "expo run:android --device Pixel_7",
    "android:pad": "expo run:android --device 2410CRP4CG",
    "android:oppo": "expo run:android --device CPH2399",
    "android:all": "adb devices | grep -v List | grep device | cut -f 1 | xargs -I {} npx expo run:android --device {}",
